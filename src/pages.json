{
  "pages": [
    // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/user/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "subPackages": [
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#035CAC",
    "backgroundColor": "#035CAC",
    "usingComponents": {
        "van-cell": "/wxcomponents/@vant/weapp/dist/cell/index",
        "van-cell-group": "/wxcomponents/@vant/weapp/dist/cell-group/index"
        "van-button": "/wxcomponents/@vant/weapp/dist/button/index"
        "van-grid": "/wxcomponents/@vant/weapp/dist/grid/index",
        "van-grid-item": "/wxcomponents/@vant/weapp/dist/grid-item/index"
        "van-calendar": "/wxcomponents/@vant/weapp/dist/calendar/index"
    }
  },
  "tabBar": {
    "color": "#035CAC",
    "selectedColor": "#035CAC",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/home/index",
        "iconPath": "static/images/home.png",
        "selectedIconPath": "static/images/home-filling.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/user/index",
        "iconPath": "static/images/user.png",
        "selectedIconPath": "static/images/user-filling.png",
        "text": "我的"
      }
    ]
  }
}
